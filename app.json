{
  "expo": {
    "name": "Pet Carer",
    "slug": "pet-carer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pet-carer-logo-resized.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/pet-carer-logo-resized.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffc966"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.petcarer.petcarer"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/pet-carer-logo-resized.png",
        "backgroundColor": "#ffc966"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.petcarer.petcarer"
    },
    "scheme": "com.petcarer.petcarer",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6f26cc0a-0d6d-49ac-9c16-66b98ad3d9a4"
      }
    },
    "owner": "pet-carer"
  }
}
